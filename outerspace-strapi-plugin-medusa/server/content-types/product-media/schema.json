{
  "kind": "collectionType",
  "collectionName": "product_medias",
  "info": {
    "singularName": "product-media",
    "pluralName": "product-medias",
    "displayName": "M | Product Media",
    "description": "To store product related multimedia files"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    },
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "medusa_id": {
      "type": "uid",
      "required": true
    },
    "filename": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "media_url": {
      "type": "text",
      "multiple": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "metadata": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::outerspace-strapi-plugin-medusa.product",
      "inversedBy": "product_medias",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    }
  }
}
